#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'galvani==0.2.1','console_scripts','res2sqlite'
__requires__ = 'galvani==0.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('galvani==0.2.1', 'console_scripts', 'res2sqlite')()
    )
